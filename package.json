{
  "name": "smart-distancing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npx jetify && cd ios && sh enableBleRCTSystemSetting.sh",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "coverage": "nyc jest && nyc report --reporter=html && open coverage/index.html || xdg-open coverage/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "adb shell input keyevent 82",
    "generate-flatbuffers": "sh App/Schemas/generate_flatbuffers.sh && standard --fix 'App/Schemas/*.js'",
    "e2eTest": "./e2e/runTest.sh",
    "flow:coverage": "flow-coverage-report",
    "ios:bundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run flow",
      "pre-push": "lint-staged && yarn run flow && jest"
    }
  },
  "lint-staged": {
    "*.js": "standard --verbose"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.3",
    "@react-native-community/datetimepicker": "^2.3.2",
    "@react-native-community/google-signin": "^3.0.3",
    "@react-native-community/netinfo": "^4.6.1",
    "@react-native-community/push-notification-ios": "^1.0.5",
    "@react-native-firebase/analytics": "^6.1.0",
    "@react-native-firebase/app": "^6.1.0",
    "@react-native-firebase/crashlytics": "^6.1.0",
    "apisauce": "^1.1.1",
    "axios": "^0.19.0",
    "buffer": "^5.4.3",
    "convert-units": "^2.3.4",
    "d3-scale": "^3.2.1",
    "d3-shape": "^1.3.7",
    "eslint": "^6.7.1",
    "flatbuffers": "^1.11.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "lodash.debounce": "^4.0.8",
    "mathjs": "^6.2.5",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "native-base": "^2.13.8",
    "nyc": "^14.1.1",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-native": "0.61.2",
    "react-native-ble-plx": "^1.1.0",
    "react-native-calendars": "^1.265.0",
    "react-native-check-box": "^2.1.7",
    "react-native-collapsible": "^1.5.1",
    "react-native-config": "^0.12.0",
    "react-native-copilot": "^2.4.6",
    "react-native-deprecated-custom-components": "^0.1.2",
    "react-native-device-info": "^2.3.2",
    "react-native-elements": "^1.2.7",
    "react-native-fbsdk": "^1.1.2",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-i18n": "^2.0.15",
    "react-native-keep-awake": "^4.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^1.3.1",
    "react-native-offline": "^5.2.0",
    "react-native-paper": "^2.16.0",
    "react-native-permissions": "^2.0.4",
    "react-native-progress": "^3.6.0",
    "react-native-push-notification": "^3.1.9",
    "react-native-reanimated": "^1.7.0",
    "react-native-render-html": "^4.1.2",
    "react-native-router-flux": "^4.2.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-secure-key-store": "^2.0.5",
    "react-native-snap-carousel": "^4.0.0-beta.2",
    "react-native-spinkit": "^1.5.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "9.13.3",
    "react-native-switch-pro": "^1.0.4",
    "react-native-system-setting": "^1.7.3",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^8.0.3",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.1.1",
    "seamless-immutable": "^7.1.4"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ],
    "ignore": [
      "App/Schemas/*_generated.js"
    ]
  },
  "flow-coverage-report": {
    "globIncludePatterns": [
      "App/Components/**/*.js",
      "App/Components/*.js",
      "App/Containers/**/*.js",
      "App/Containers/*.js",
      "App/Helpers/*.js",
      "App/Redux/*.js",
      "App/Sagas/*.js",
      "App/Sagas/**/*.js",
      "App/Services/*.js"
    ],
    "reportTypes": [
      "text",
      "html",
      "json"
    ]
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "globals": {
      "__DEV__": true
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/jestconfig.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-flow": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "detox": "15.4.2",
    "flow-bin": "^0.113.0",
    "flow-coverage-report": "^0.6.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "react-test-renderer": "16.12.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}